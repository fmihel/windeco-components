{
  "name": "fmihel-windeco-components",
  "version": "1.7.1",
  "description": "components for orders windeco",
  "main": "dist/windeco-components.min.js",
  "scripts": {
    "run-dev-server": "webpack-dev-server --config webpack.config.dev.js",
    "build": "webpack && webpack --prod",
    "build-to": "npm run build-to-order && npm run build-to-order-header && npm run build-to-order-karniz",
    "build-to-order": "webpack --too && npm run copy-style-to-order",
    "build-to-order-header": "webpack --tooh && npm run copy-style-to-order-header",
    "build-to-order-karniz": "webpack --took && npm run copy-style-to-order-karniz",
    "build-to-order-blank": "webpack --toob && npm run copy-style-to-order-blank",
    "copy-style-to-order-header": "cp -r style ../order-header/node_modules/fmihel-windeco-components/",
    "copy-style-to-order-karniz": "cp -r style ../order-karniz/node_modules/fmihel-windeco-components/",
    "copy-style-to-order": "cp -r style ../order/node_modules/fmihel-windeco-components/",
    "copy-style-to-order-blank": "cp -r style ../order-blank/node_modules/fmihel-windeco-components/",
    "git-push": "git push windeco-components && git push windeco-components --tag",
    "npm-publish-patch": "npm run build && npm version patch && npm publish && npm run git-push",
    "npm-publish-minor": "npm run build && npm version minor && npm publish && npm run git-push",
    "npm-publish-major": "npm run build && npm version major && npm publish && npm run git-push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmihel/windeco-components.git"
  },
  "keywords": [
    "react",
    "components",
    "javascript"
  ],
  "author": "fmihel76@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fmihel/windeco-components/issues"
  },
  "homepage": "https://github.com/fmihel/windeco-components#readme",
  "peerDependencies": {
    "fmihel-polyfills": ">=0.2.0",
    "fmihel-browser-lib": "^1.15.0",
    "jquery": "^3.4.1",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "prop-types": "^15.7.2",
    "lodash": "^4.17.16"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "autoprefixer": "^9.8.8",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "bootstrap": "^4.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "exports-loader": "^0.7.0",
    "fmihel-browser-lib": "^2.3.1",
    "fmihel-polyfills": "^0.3.0",
    "fmihel-redux-wrapper": "^2.0.0",
    "fmihel-server-lib": "^0.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.16",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  }
}
